<p id="notice"><%= notice %></p>


<h2>CLIENTE REGISTRADO<h2/>


<table>
<tr>
<td><b>Nombre:</b></td>
<td><%= @client.name %></td>
<td rowspan="6"> <%= image_tag @client.avatar.url %></td>
</tr>
<tr>
<td><b>Email:</b></td>
<td><%= @client.email %></td>
</tr>
<tr>
<td><b>Direccion:</b></td>
<td><%= @client.address %></td>
</tr>
<tr>
<td><b>Telefono:</b></td>
<td><%= @client.phonehome %></td>
</tr>
<tr>
<td><b>Celular:</b></td>
<td><%= @client.phonecel%></td>
</tr>
</table>

<br />
<%= link_to 'Editar', edit_client_path(@client) %> |
<%= link_to 'Atras', clients_path %>

