<h1>Listing prospectus</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Imag</th>
    <th>Phonehome</th>
    <th>Phonecelular</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prospectus.each do |prospectu| %>
  <tr>
    <td><%= prospectu.name %></td>
    <td><%= prospectu.email %></td>
    <td><%= prospectu.imag %></td>
    <td><%= prospectu.phonehome %></td>
    <td><%= prospectu.phonecelular %></td>
    <td><%= link_to 'Show', prospectu %></td>
    <td><%= link_to 'Edit', edit_prospectu_path(prospectu) %></td>
    <td><%= link_to 'Destroy', prospectu, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

