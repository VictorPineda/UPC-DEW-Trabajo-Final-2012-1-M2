<h1>Lista de Prospectos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Email</th>
    <th>Imagen</th>
    <th>Telefono</th>
    <th>Celular</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @prospectus.each do |prospectu| %>
  <tr>
    <td><%= prospectu.name %></td>
    <td><%= prospectu.email %></td>
    <td><%= prospectu.imag %></td>
    <td><%= prospectu.phonehome %></td>
    <td><%= prospectu.phonecelular %></td>
    <td><%= link_to 'Ver', prospectu %></td>
    <td><%= link_to 'Editar', edit_prospectu_path(prospectu) %></td>
    <td><%= link_to 'Eliminar', prospectu, confirm: 'Esta seguro que desea eliminar?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Prospecto', new_prospectu_path %>
